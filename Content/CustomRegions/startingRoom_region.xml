<customRegion name="StartingRoom" floorColor="#de663e" borderColor="#f2b566" sample="roomSample1">
	<location>
	</location>
	<dimensions>
		<rectangle width="19" height="13" />
	</dimensions>
	<content>

		<entrance name="MainEntrance" number="1" />

		<creature name="Hero" number="1">
			<location>
				<layer where="center" />
			</location>
		</creature>

		<structure name="YellowDoor" number="3">
			<location>
				<proximity to="MainEntrance" distance="very close" />
			</location>
		</structure>

		<structure name="BookStand" number="4-5">
			<location>
				<layer where="border" />
				<proximity to="MainEntrance" distance="very far" tolerance="4" />
			</location>
		</structure>

		<item name="YellowKey" number="3"></item>

		<item name="HealthPotion" number="1"></item>

		<item name="Slingshot" number="1"></item>
		
	</content>
</customRegion>
