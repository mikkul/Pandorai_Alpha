<customRegion name="InitialForest" floorColor="#4b9c50" borderColor="#2c693d" sample="forestWithPathSample">
	<location>
	</location>
	<dimensions>
		<rectangle width="35-45" height="30-40" fillInterior="false">
			<rectangle width="14" height="14" placement="center" offsetX="0" offsetY="0" />
		</rectangle>
	</dimensions>
	<content>

		<structure name="Chest" number="1">
			<location>
				<layer where="center" />
			</location>
			<inventory>
				<invItem name="HealthPotion" number="2-3" />
				<invItem name="ManaPotion" number="1" />
			</inventory>
		</structure>

		<structure name="Barrel" number="8-11">
			<inventory>
				<invItem name="HealthPotion" number="1" chance="50" />
			</inventory>
		</structure>

		<choice>
			<item name="MediumEndurancePotion" number="1"></item>
			<item name="MediumEnergyPotion" number="1"></item>
			<item name="MediumStrengthPotion" number="1"></item>
		</choice>

		<structure name="Campfire" number="5-7"></structure>

		<creature name="Spider" number="5">
			<location>
			</location>
		</creature>

		<creature name="Wolf" number="4">
			<location>
				<layer where="center" offset="4" />
			</location>
		</creature>

		<creature name="Wolf" number="4">
			<location>
				<layer where="center" offset="7" />
			</location>
		</creature>

		<creature name="Wolf" number="4">
		</creature>		

		<creature name="Elder" number="1">
			<location>
				<layer where="center" />
			</location>
		</creature>

		<creature name="Seeker" number="1">
			<location>
				<layer where="center" />
			</location>
		</creature>		

		<structure name="Grave" number="1"></structure>
		<item name="FireArrowSpell" number="1">
			<location>
				<proximity to="Grave" distance="very close" />
			</location>
		</item>
		<item name="IceArrowSpell" number="1">
			<location>
				<proximity to="Grave" distance="very close" />
			</location>
		</item>

	</content>
</customRegion>
